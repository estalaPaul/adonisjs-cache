{
    "scripts": {
        "mrm": "mrm --preset=@adonisjs/mrm-preset",
        "lint": "eslint . --ext=.ts",
        "pretest": "npm run lint",
        "test": "c8 node -r @adonisjs/require-ts/build/register bin/test.ts",
        "clean": "del-cli build",
        "compile": "npm run lint && npm run clean && tsc && npm run copy-templates",
        "commit": "git-cz",
        "build": "npm run compile",
        "prepublishOnly": "npm run build",
        "copy-templates": "cp -r templates build/"
    },
    "devDependencies": {
        "@adonisjs/core": "^5.0.4-preview-rc-2.1",
        "@adonisjs/mrm-preset": "^5.0.3",
        "@adonisjs/require-ts": "^2.0.10",
        "@japa/expect": "^1.1.3",
        "@japa/runner": "^2.0.5",
        "@japa/spec-reporter": "^1.1.11",
        "@poppinss/dev-utils": "^2.0.2",
        "@sinonjs/fake-timers": "^9.1.1",
        "@types/node": "^17.0.23",
        "@types/sinonjs__fake-timers": "^8.1.2",
        "c8": "^7.11.0",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "del-cli": "^4.0.1",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-adonis": "^2.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "mrm": "^4.0.0",
        "prettier": "2.6.1",
        "typescript": "^4.6.3"
    },
    "name": "adonisjs-cache",
    "version": "0.3.1",
    "description": "Cache provider for AdonisJS",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/estalaPaul/adonisjs-cache.git"
    },
    "keywords": [
        "AdonisJS",
        "Cache",
        "File cache"
    ],
    "author": "Paul Estala",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/estalaPaul/adonisjs-cache/issues"
    },
    "homepage": "https://github.com/estalaPaul/adonisjs-cache#readme",
    "files": [
        "build/adonis-typings",
        "build/bin",
        "build/providers",
        "build/src",
        "build/templates",
        "build/test-helpers",
        "build/tests"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "types": "build/adonis-typings/index.d.ts",
    "adonisjs": {
        "types": "adonisjs-cache",
        "providers": [
            "adonisjs-cache"
        ]
    }
}
