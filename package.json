{
    "scripts": {
        "mrm": "mrm --preset=@adonisjs/mrm-preset",
        "lint": "eslint . --ext=.ts",
        "pretest": "npm run lint",
        "test": "npx nyc -a --include 'src/**/*' node -r @adonisjs/require-ts/build/register bin/test.ts",
        "clean": "del-cli build",
        "compile": "npm run lint && npm run clean && tsc && npm run copy-templates",
        "commit": "git-cz",
        "release": "np",
        "build": "npm run compile",
        "prepublishOnly": "npm run build",
        "copy-templates": "cp -r templates build/"
    },
    "devDependencies": {
        "@adonisjs/core": "^5.7.6",
        "@adonisjs/mrm-preset": "^5.0.3",
        "@adonisjs/require-ts": "^2.0.11",
        "@japa/expect": "^1.1.4",
        "@japa/runner": "^2.0.7",
        "@japa/spec-reporter": "^1.1.12",
        "@poppinss/dev-utils": "^2.0.3",
        "@sinonjs/fake-timers": "^9.1.2",
        "@types/node": "^17.0.31",
        "@types/sinonjs__fake-timers": "^8.1.2",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "del-cli": "^4.0.1",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-adonis": "^2.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "mrm": "^4.0.0",
        "np": "^7.6.1",
        "nyc": "^15.1.0",
        "prettier": "2.6.2",
        "typescript": "^4.6.4"
    },
    "name": "adonisjs-cache",
    "version": "1.0.2",
    "description": "Cache provider for AdonisJS",
    "main": "build/providers/CacheProvider.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/estalaPaul/adonisjs-cache.git"
    },
    "keywords": [
        "AdonisJS",
        "Cache",
        "File cache"
    ],
    "author": "Paul Estala",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/estalaPaul/adonisjs-cache/issues"
    },
    "homepage": "https://github.com/estalaPaul/adonisjs-cache#readme",
    "files": [
        "build/adonis-typings",
        "build/providers",
        "build/src",
        "build/templates"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "types": "build/adonis-typings/index.d.ts",
    "adonisjs": {
        "types": "adonisjs-cache",
        "providers": [
            "adonisjs-cache"
        ],
        "templates": {
            "basePath": "./build/templates",
            "config": [
                {
                    "src": "config.txt",
                    "dest": "cache"
                }
            ]
        }
    },
    "np": {
        "contents": ".",
        "anyBranch": false
    }
}
